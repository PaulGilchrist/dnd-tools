    "bootstrap": "~5.0.2",
    "rxjs": "~6.6.0",
    "showdown": "~1.9.1",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "^14.2.10",
